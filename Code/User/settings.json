{
    "workbench.sideBar.location": "left",
    "explorer.confirmDragAndDrop": false,
    "breadcrumbs.enabled": true,

    "python.formatting.autopep8Args": ["--max-line-length=500"],
    "python.linting.pylintArgs": [
        "--variable-rgx=[A-Za-z_][A-Za-z0-9_]*$",
        "--function-rgx=[A-Za-z_][A-Za-z0-9_]*$",
        "--argument-rgx=[A-Za-z_][A-Za-z0-9_]*$",
        "--extension-pkg-whitelist=cv2",
        "--errors-only"
    ],
    "editor.formatOnSave": false,
    "git.enableSmartCommit": true,
    "liveSassCompile.settings.showOutputWindow": false,
    "git.autofetch": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.iconTheme": "vscode-icons",
    "prettier.tabWidth": 4,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "liveServer.settings.donotVerifyTags": true,
    "python.jediEnabled": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "extensions.recommendations": [
        "aaron-bond.better-comments",
        "abusaidm.html-snippets",
        "almenon.arepl",
        "bmewburn.vscode-intelephense-client",
        "christian-kohler.npm-intellisense",
        "CoenraadS.bracket-pair-colorizer",
        "cstrap.flask-snippets",
        "dariofuzinato.vue-peek",
        "DotJoshJohnson.xml",
        "eamodio.gitlens",
        "ecmel.vscode-html-css",
        "eg2.vscode-npm-script",
        "emmanuelbeziat.vscode-great-icons",
        "esbenp.prettier-vscode",
        "fabianlauer.vs-code-xml-format",
        "febean.vue-format",
        "formulahendry.auto-rename-tag",
        "hollowtree.vue-snippets",
        "humao.rest-client",
        "jcbuisson.vue",
        "jolaleye.horizon-theme-vscode",
        "KnisterPeter.vscode-github",
        "letrieu.expand-region",
        "liuji-jim.vue",
        "lonefy.vscode-JS-CSS-HTML-formatter",
        "mblode.pretty-formatter",
        "ms-azuretools.vscode-azureappservice",
        "ms-azuretools.vscode-azureresourcegroups",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter",
        "ms-vscode-remote.remote-wsl",
        "ms-vscode.azure-account",
        "ms-vscode.cpptools",
        "njpwerner.autodocstring",
        "octref.vetur",
        "peakchen90.vue-beautify",
        "pranaygp.vscode-css-peek",
        "redhat.vscode-xml",
        "ritwickdey.live-sass",
        "ritwickdey.LiveServer",
        "sdras.night-owl",
        "sdras.vue-vscode-extensionpack",
        "sdras.vue-vscode-snippets",
        "shenjiaolong.vue-helper",
        "srfrnk.user-extensions",
        "syler.sass-indented",
        "tht13.python",
        "tombonnike.vscode-status-bar-format-toggle",
        "vscode-icons-team.vscode-icons",
        "WASTeamAccount.WebTemplateStudio-dev-nightly",
        "xabikos.JavaScriptSnippets",
        "zhuangtongfa.material-theme",
        "Zignd.html-css-class-completion",
        "znck.vue"
    ],
    "python.languageServer": "Pylance",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
    },
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "files.associations": {
        "*.html": "html"
    },
    "tabnine.experimentalAutoImports": true,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "One Dark Pro"

}